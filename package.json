{
  "name": "murasaki-random-file-generator-cli",
  "version": "1.9.0",
  "description": "A simple file generator that uses brute force to create files of a certain size.",
  "scripts": {
    "clean-dist": "node_modules/.bin/rimraf ./dist",
    "build": "npm run clean-dist && tsc && npm run copypackage",
    "copypackage": "cpx package.json dist/src",
    "dist": "npm run build && cd dist/src && npm publish"
  },
  "keywords": [
    "typescript",
    "tsc",
    "random",
    "file",
    "generator",
    "images",
    "test data"
  ],
  "dependencies": {
    "@azure/functions": "^1.2.2",
    "@types/node": "^14.0.14",
    "child_process": "^1.0.2",
    "commander": "^5.1.0",
    "murasaki-random-file-generator": "^1.6.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node-fetch": "^2.5.7",
    "@types/sharp": "^0.25.0",
    "@types/sinon": "^9.0.4",
    "@types/yargs": "^15.0.5",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "cpx": "^1.5.0"
  },
  "files": [
    "*"
  ],
  "main": "main.js",
  "repository": {
    "directory": "",
    "type": "git",
    "url": "https://github.com/Murasaki/random-file-generator-cli"
  },
  "bin": {
    "generate-file": "main.js"
  }
}
